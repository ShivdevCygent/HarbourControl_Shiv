@model List<HarbourControlModels.BoatModel>
@{
    ViewBag.Title = "Harbour Control Home";
    int i = 0;
}

<form method="post" action="@Url.Action("index")">

    <div class="col-lg-12">
        <h1 id="WindSpeedDisplay"></h1>
    </div>
    <table class="table">
        <tr>
            <th width="" style="width:200px">Boat Type</th>
            <th style="width:100px">
                Boat Speed
            </th>
            <th style="width:100px">
                Journey Start Time
            </th>
            <th style="width:120px"></th>
            <th>
                Travelling Distance Update
            </th>
            <th style="width:120px"></th>
            <th style="width:100px">
                Journey End Time
            </th>
        </tr>
        @foreach (var item in Model)
        {
    <tr>
        <td class="boatType_@i">@item.Type</td>

        <td class="boatSpeed_@i" value="@item.Speed">@item.Speed</td>

        <td class="boatJourneyStart_@i" value=""></td>

        <td>
            <img src="data:image/jpeg;base64," width="120px" />
        </td>

        <td>
            <progress class="boatTravel_@i" value="0" max="100"> @item.Perimeter </progress>
        </td>

        <td>
            <img src="data:image/jpeg;base64," width="120px"/>
        </td>
        <td class="boatJourneyEnd_@i" value=""></td>
    </tr>
            i++;
        }
    </table>
</form>